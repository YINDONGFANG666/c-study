NO.1
#include<stdio.h>
main()
{
	FILE*rf;
	int a[5];
	rf=fopen("c:\\ydf.txt","r");
	for(int i=0;i<5;i++)
	{
		fscanf(rf,"%d",&a[i]);
		printf("%d\n",a[i]);
	}
}
NO.2
#include<stdio.h>
main()
{
	FILE*rf;
	int a[5][5];
	int i,j;
	rf=fopen("c:\\ydf.txt","r");
	for(i=0;i<5;i++)
	{
		for(j=0;j<5;j++)
		{
			fscanf(rf,"%d%d",&a[i][j]);
			printf("%d%d\n",a[i][j]);
		}
	}
}
